{
	"compilerOptions": {
		"target": "es2016",
		"jsx": "preserve",
		"module": "commonjs",
		"allowJs": true,
		"outDir": "./dist",
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"skipLibCheck": true,
		"strictNullChecks": false
	},
	"exclude": ["client", "src", "dist", "node_modules"]
}
